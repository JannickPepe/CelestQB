generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Guide {
  id    Int      @id @default(autoincrement())
  title String
  text  String
  date  DateTime
  image String?
}

model Tip {
  id    Int      @id @default(autoincrement())
  title String
  text  String
  date  DateTime
  image String?
}
